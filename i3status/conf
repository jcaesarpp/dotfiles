general {
        colors = true
        interval = 5
}

order += "battery all"
order += "tztime local"
#order += "volume master"

tztime local {
        format = "%b %d, %a %H:%M"
}

battery all {
        format = "%status %percentage %remaining"
}

volume master {
        format = "%volume"
        format_muted = "Muted"
        device = "default"
        mixer = "Headphone"
        mixer_idx = 0
}

